	<system>// Place
//Process = Template();

// List one or more processes to be composed into a system.
system client, db;</system>
	<queries>
		<query>
			<formula>A[] not client.serReceiving or (x &lt;= 4)
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
